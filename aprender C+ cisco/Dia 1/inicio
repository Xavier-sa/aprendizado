// Example program
#include <iostream>
#include <string>

int main()
{
  std::string name;
  std::cout << "What is your name? ";
  getline (std::cin, name);
  std::cout << "Hello, " << name << "!\n";


  std::string curso;
  std::cout << "What is Course? ";
  getline (std::cin,curso);
  std::cout << "Seu curso e," << curso << "!\n";
}